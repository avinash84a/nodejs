	<% include ../layout/header.ejs %>


		<% var data= JSON.parse(users) %>

		<div class="container-full">
			<div class="row">
				<div class="col-md-offset-1">
					<div class="col-md-10">

						<table class="table table-bordered table-hover">	
		<thead>
			<th>ID</th>
			<th>FullName</th>
			<th>Email</th>
			<th>Mobile</th>
			<th>Action</th>

		</thead>
		<% for(var i=0; i < data.length; i++) { %>
		<tr>
 		<td><%= data[i].id %></td>
 		<td><%= data[i].full_name %></td>
 		<td><%= data[i].email %></td>
 		<td><%= data[i].mobile %></td>
 		<td> <a href="/update?id=<%= data[i].id %>" class="btn btn-primary">Update</a>
			 <a href="/delete?id=<%= data[i].id %>"class="btn btn-danger">Delete</a>
		</td>
		 </tr>
		<% } %>
	</table>
					</div>
				</div>
			</div>
		</div>
	
<% include ../layout/footer.ejs %>
